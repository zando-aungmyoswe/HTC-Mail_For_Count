<odoo>
  <data>
    <record model="ir.ui.view" id="view_floor_form">
      <field name="name">Floor Form</field>
      <field name="model">ml.floor</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <group>
              <field name="name"/>
              <field name="code"/>
              </group>
              <group>
              </group>
              <group colspan="2">
             <!--  <field name="remark"/> -->
              </group>
            </group>
          </sheet>
        </form>
      </field>
    </record>
    <record model="ir.ui.view" id="view_floor_tree">
      <field name="name">Floor list</field>
      <field name="model">ml.floor</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="code"/>
         <!--  <field name="remark"/> -->
        </tree>
      </field>
    </record>
     <record model="ir.ui.view" id="view_floor_filter">
      <field name="name">Floor list</field>
      <field name="model">ml.floor</field>
      <field name="arch" type="xml">
        <search>
          <field name="name"/>
          <field name="code"/>
        </search>
      </field>
    </record>
     <record id="action_floor_all" model="ir.actions.act_window">
       <field name="name">Floor</field>
       <field name="res_model">ml.floor</field>
       <field name="view_type">form</field>
       <field name="view_mode">tree,form</field>
       <field name="view_id" ref="view_floor_tree"/> 
    </record> 
    <menuitem  id="floor_menu_root_sub" 
      name="Floor" 
      parent="mall_menu_root" 
      sequence="2"/>
    <menuitem  id="floor_menu_root" 
      name="Floor" 
      parent="floor_menu_root_sub"
      action="action_floor_all"
      sequence="1"/>
  </data>
</odoo>